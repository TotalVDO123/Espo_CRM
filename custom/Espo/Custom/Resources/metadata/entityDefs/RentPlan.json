{
    "fields": {
        "name": {
            "type": "varchar",
            "required": true,
            "pattern": "$noBadCharacters"
        },
        "description": {
            "type": "text"
        },
        "createdAt": {
            "type": "datetime",
            "readOnly": true
        },
        "modifiedAt": {
            "type": "datetime",
            "readOnly": true
        },
        "createdBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "modifiedBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "assignedUser": {
            "type": "link",
            "required": true,
            "view": "views/fields/assigned-user"
        },
        "teams": {
            "type": "linkMultiple",
            "view": "views/fields/teams"
        },
        "contact": {
            "type": "link",
            "required": true
        },
        "price": {
            "type": "currency",
            "required": true,
            "isCustom": true
        },
        "isPaid": {
            "notNull": true,
            "type": "bool",
            "isCustom": true
        },
        "duration": {
            "type": "int",
            "required": true,
            "min": 0,
            "isCustom": true
        },
        "startDate": {
            "notNull": false,
            "type": "date",
            "required": true,
            "default": "javascript: return this.dateTime.getToday();",
            "tooltip": true,
            "isCustom": true
        },
        "classCount": {
            "type": "int",
            "min": 0,
            "tooltip": true,
            "isCustom": true
        },
        "discount": {
            "type": "link"
        },
        "rents": {
            "type": "linkMultiple",
            "layoutDetailDisabled": true,
            "layoutMassUpdateDisabled": true,
            "layoutListDisabled": true,
            "noLoad": true,
            "importDisabled": true,
            "exportDisabled": true,
            "customizationDisabled": true,
            "isCustom": true
        }
    },
    "links": {
        "createdBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "modifiedBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "assignedUser": {
            "type": "belongsTo",
            "entity": "User"
        },
        "teams": {
            "type": "hasMany",
            "entity": "Team",
            "relationName": "entityTeam",
            "layoutRelationshipsDisabled": true
        },
        "contact": {
            "type": "belongsTo",
            "foreign": "rentPlans",
            "entity": "Contact",
            "audited": false,
            "isCustom": true
        },
        "discount": {
            "type": "belongsTo",
            "foreign": "rentPlans",
            "entity": "Discount",
            "audited": false,
            "isCustom": true
        },
        "rents": {
            "type": "hasMany",
            "foreign": "rentPlan",
            "entity": "Rent",
            "audited": false,
            "isCustom": true
        }
    },
    "collection": {
        "orderBy": "createdAt",
        "order": "desc",
        "textFilterFields": [
            "name"
        ],
        "fullTextSearch": false,
        "countDisabled": false
    },
    "indexes": {
        "name": {
            "columns": [
                "name",
                "deleted"
            ]
        },
        "assignedUser": {
            "columns": [
                "assignedUserId",
                "deleted"
            ]
        },
        "createdAt": {
            "columns": [
                "createdAt"
            ]
        },
        "createdAtId": {
            "unique": true,
            "columns": [
                "createdAt",
                "id"
            ]
        }
    },
    "optimisticConcurrencyControl": false
}