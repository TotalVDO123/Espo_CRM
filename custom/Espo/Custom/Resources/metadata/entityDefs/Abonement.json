            "readOnly": true,
            "tooltip": false,
            "isCustom": true
        },
        "isActive": {
            "notNull": true,
            "type": "bool",
            "inlineEditDisabled": true,
            "tooltip": true,
            "isCustom": true,
            "readOnly": true
        },
        "isPaid": {
            "notNull": true,
            "type": "bool",
            "isCustom": true,
            "default": false
        },
        "price": {
            "type": "currency",
            "readOnly": true,
            "isCustom": true
        },
        "startDate": {
            "notNull": false,
            "type": "date",
            "readOnly": false,
            "tooltip": false,
            "isCustom": true,
            "default": "javascript: return this.dateTime.getToday();"
        },
        "abonplan": {
            "type": "link",
            "defaultAttributes": null,
            "required": true
        },
        "contact": {
            "type": "link",
            "required": true
        },
        "group": {
            "type": "link"
        },
        "groups": {
            "type": "linkMultiple",
            "layoutDetailDisabled": false,
            "layoutMassUpdateDisabled": false,
            "layoutListDisabled": false,
            "noLoad": false,
            "importDisabled": false,
            "exportDisabled": false,
            "customizationDisabled": false,
            "isCustom": true,
            "defaultAttributes": null,
            "required": true
        },
        "marks": {
            "type": "linkMultiple",
            "layoutDetailDisabled": true,
            "layoutMassUpdateDisabled": true,
            "layoutListDisabled": true,
            "noLoad": true,
            "importDisabled": true,
            "exportDisabled": true,
            "customizationDisabled": true,
            "isCustom": true
        },
        "isInitialized": {
            "notNull": true,
            "type": "bool",
            "tooltip": true,
            "isCustom": true
        },
        "note": {
            "type": "text",
            "rowsMin": 2,
            "cutHeight": 200,
            "maxLength": 120,
            "isCustom": true,
            "audited": true
        },
        "number": {
            "type": "number",
            "len": 36,
            "notNull": false,
            "unique": false,
            "nextNumber": 1,
            "padLength": 4,
            "copyToClipboard": true,
            "isCustom": true
        },
        "discount": {
            "type": "link"
        },
        "isActivated": {
            "notNull": true,
            "type": "bool",
            "tooltip": true,
            "isCustom": true
        },
        "activationDateTime": {
            "notNull": false,
            "type": "datetime",
            "useNumericFormat": true,
            "readOnly": true,
            "minuteStep": 30,
            "tooltip": false,
            "isCustom": true
        },
        "isFreezed": {
            "notNull": true,
            "type": "bool",
            "readOnly": false,
            "tooltip": true,
            "isCustom": true,
            "audited": true
