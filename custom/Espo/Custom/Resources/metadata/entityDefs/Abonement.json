            "type": "belongsTo",
            "entity": "User"
        },
        "modifiedBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "assignedUser": {
            "type": "belongsTo",
            "entity": "User"
        },
        "teams": {
            "type": "hasMany",
            "entity": "Team",
            "relationName": "entityTeam",
            "layoutRelationshipsDisabled": true
        },
        "abonplan": {
            "type": "belongsTo",
            "foreign": "abonements",
            "entity": "Abonplan",
            "audited": false,
            "isCustom": true
        },
        "contact": {
            "type": "belongsTo",
            "foreign": "abonements",
            "entity": "Contact",
            "audited": true,
            "isCustom": true
        },
        "group": {
            "type": "belongsTo",
            "foreign": "abonements",
            "entity": "Group",
            "audited": false,
            "isCustom": true
        },
        "groups": {
            "type": "hasMany",
            "relationName": "abonementGroup",
            "foreign": "abonementsMeny",
            "entity": "Group",
            "audited": false,
            "isCustom": true
        },
        "marks": {
            "type": "hasMany",
            "foreign": "abonement",
            "entity": "Mark",
            "audited": false,
            "isCustom": true
        },
        "discount": {
            "type": "belongsTo",
            "foreign": "abonements",
            "entity": "Discount",
            "audited": false,
            "isCustom": true
        }
