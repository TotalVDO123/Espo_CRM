{
    "fields": {
        "name": {
            "type": "varchar",
            "required": false,
            "pattern": "$noBadCharacters",
            "readOnly": true,
            "options": []
        },
        "description": {
            "type": "text"
        },
        "createdAt": {
            "type": "datetime",
            "readOnly": true
        },
        "modifiedAt": {
            "type": "datetime",
            "readOnly": true
        },
        "createdBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "modifiedBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "assignedUser": {
            "type": "link",
            "required": false,
            "view": "views/fields/assigned-user",
            "readOnly": true,
            "tooltip": true
        },
        "teams": {
            "type": "linkMultiple",
            "view": "views/fields/teams",
            "required": true
        },
        "group": {
            "type": "link",
            "required": true
        },
        "hall": {
            "type": "link",
            "required": true
        },
        "startTime": {
            "type": "varchar",
            "required": true,
            "options": [
                "15:00",
                "16:00",
                "17:00",
                "18:00",
                "19:00",
                "20:00"
            ],
            "tooltip": true,
            "isCustom": true
        },
        "endTime": {
            "type": "varchar",
            "required": true,
            "maxLength": 150,
            "options": [
                "15:00",
                "16:00",
                "17:00",
                "18:00",
                "19:00",
                "20:00"
            ],
            "tooltip": true,
            "isCustom": true
        },
        "trainerId": {
            "type": "varchar",
            "maxLength": 150,
            "options": [],
            "tooltip": true,
            "isCustom": true,
            "readOnly": false
        },
        "weekDay": {
            "type": "enum",
            "options": [
                "monday",
                "tuesday",
                "wednesday",
                "thursday",
                "friday",
                "saturday",
                "sunday"
            ],
            "style": {
                "monday": null,
                "tuesday": null,
                "wednesday": null,
                "thursday": null,
                "friday": null,
                "saturday": null,
                "sunday": null
            },
            "default": "monday",
            "isCustom": true,
            "required": true,
            "inlineEditDisabled": false
        },
        "weekTemplate": {
            "type": "link",
            "required": true
        },
        "assignedTeams": {
            "type": "array",
            "storeArrayValues": true,
            "noEmptyString": true,
            "tooltip": true,
            "isCustom": true
        }
    },
    "links": {
        "createdBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "modifiedBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "assignedUser": {
            "type": "belongsTo",
            "entity": "User"
        },
        "teams": {
            "type": "hasMany",
            "entity": "Team",
            "relationName": "entityTeam",
            "layoutRelationshipsDisabled": true
        },
        "group": {
            "type": "belongsTo",
            "foreign": "trainingTemplates",
            "entity": "Group",
            "audited": false,
            "isCustom": true
        },
        "hall": {
            "type": "belongsTo",
            "foreign": "trainingTemplates",
            "entity": "Hall",
            "audited": false,
            "isCustom": true
        },
        "weekTemplate": {
            "type": "belongsTo",
            "foreign": "trainingTemplates",
            "entity": "WeekTemplate",
            "audited": false,
            "isCustom": true
        }
    },
    "collection": {
        "orderBy": "createdAt",
        "order": "desc",
        "textFilterFields": [
            "name"
        ],
        "fullTextSearch": false,
        "countDisabled": false
    },
    "indexes": {
        "name": {
            "columns": [
                "name",
                "deleted"
            ]
        },
        "assignedUser": {
            "columns": [
                "assignedUserId",
                "deleted"
            ]
        },
        "createdAt": {
            "columns": [
                "createdAt"
            ]
        },
        "createdAtId": {
            "unique": true,
            "columns": [
                "createdAt",
                "id"
            ]
        }
    },
    "optimisticConcurrencyControl": false
}