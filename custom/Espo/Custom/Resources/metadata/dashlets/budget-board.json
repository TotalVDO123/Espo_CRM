{
    "view": "custom:views/dashlets/budget-board",
    "aclScope": "Account",
    "options": {
        "fields": {
            "title": {
                "type": "varchar"
            },
            "autorefreshInterval": {
                "type": "enumFloat",
                "options": [0, 0.5, 1, 2, 5, 10]
            },
            "entityName": {
                "type": "enum",
                "options": ["Абонементи", "Абонементи разові", "Абонементи пробні", "Індиви", "Оренда разова", "Оренда планова", "Товари"]
            },
            "teams": {
                "type": "array"
            }
        },
        "defaults": {
            "title": "Абонементи",
            "entityName": "Абонементи"
        },
        "layout": [
            {
                "rows": [
                    [
                        {"name": "title"},
                        {"name": "autorefreshInterval"}
                    ],
                    [
                        {"name": "entityName", "label": "Назва сутності"},
                        {"name": "teams", "label": "Команди"}
                    ]
                ]
            }
        ]
    }
}